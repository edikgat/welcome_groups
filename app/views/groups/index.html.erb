<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Created At</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @groups.each do |group| %>
      <tr>
        <td>
          <%= group.name %>
        </td>
        <td>
          <%= group.created_at %>
        </td>
        <td>
          <%= link_to "Edit", edit_group_path(group), class: "btn btn-sm btn-warning" %>
        </td>
        <td>
          <%= link_to "Enter", group_path(group), class: "btn btn-sm btn-primary" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
